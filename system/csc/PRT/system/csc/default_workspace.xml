<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.launcher">
    <home>
    
<!-- Left screen [2] -->         
<!-- Video player -->
  <appwidget
    launcher:packageName="com.sec.android.app.videoplayer"
    launcher:className="com.sec.android.app.videoplayer.appwidget.VideoWidget"
    launcher:screen="2"
    launcher:x="0"
    launcher:y="0"
    launcher:spanX="4"
    launcher:spanY="2" />                

<!-- Dropbox -->
  <favorite
    launcher:packageName="com.dropbox.android"
    launcher:className="com.dropbox.android.activity.DropboxBrowser"
    launcher:screen="2"
    launcher:x="3"
    launcher:y="3" />
              
<!-- Middle screen [3] -->             
<!-- Google Search -->
  <search
    launcher:screen="3"
    launcher:x="0"
    launcher:y="0" />

<!-- Play Box -->
  <favorite
    launcher:packageName="com.playmobile.pwf.playworld"
    launcher:className="com.playmobile.pwf.playworld.ActivityWelcome"
    launcher:screen="3"
    launcher:x="0"
    launcher:y="2" />

 <!-- Gallery -->
  <favorite
    launcher:packageName="com.sec.android.gallery3d" 
    launcher:className="com.sec.android.gallery3d.app.Gallery" 
    launcher:screen="3	" 
    launcher:x="1" 
    launcher:y="2" />

<!-- Maps -->
  <favorite
    launcher:packageName="com.google.android.apps.maps" 
    launcher:className="com.google.android.maps.MapsActivity" 
    launcher:screen="3" 
    launcher:x="2" 
    launcher:y="2" />

<!-- NaviExpert-Play -->
  <favorite
    launcher:packageName="com.naviexpert.NaviExpert_Play"
    launcher:className="org.microemu.android.MicroEmulator"
    launcher:screen="3"
    launcher:x="3"
    launcher:y="2" />


<!-- Gmail -->
  <favorite
    launcher:packageName="com.google.android.gm"
    launcher:className="com.google.android.gm.ConversationListActivityGmail"
    launcher:screen="3"
    launcher:x="0"
    launcher:y="3" />

<!-- Play Store -->
  <favorite
    launcher:packageName="com.android.vending"
    launcher:className="com.android.vending.AssetBrowserActivity"
    launcher:screen="3"
    launcher:x="1"
    launcher:y="3" />


<!-- My files -->
  <favorite
    launcher:packageName="com.sec.android.app.myfiles"
    launcher:className="com.sec.android.app.myfiles.MainActivity"
    launcher:screen="3"
    launcher:x="2"
    launcher:y="3" />

<!-- Camera -->
  <favorite
    launcher:packageName="com.sec.android.app.camera"
    launcher:className="com.sec.android.app.camera.Camera"
    launcher:screen="3"
    launcher:x="3"
    launcher:y="3" />
         
        
<!-- Right screen [4] -->
  <sactivitywidget
    launcher:packageName="com.sec.android.widgetapp.ap.hero.accuweather.widget"
    launcher:className="com.sec.android.widgetapp.ap.hero.accuweather.widget.WeatherClock"
    launcher:screen="4"
    launcher:x="0"
    launcher:y="0"
    launcher:spanX="4"
    launcher:spanY="2" />

<!-- Onet.pl -->
  <favorite
    launcher:packageName="pl.onet.onethd"
    launcher:className="pl.onet.onethd.OnetHD"
    launcher:screen="4"
    launcher:x="0"
    launcher:y="3" />

    </home>     
    
